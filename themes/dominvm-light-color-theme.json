    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords & Storage",
            "scope": [
                "keyword",
                "storage",
                "storage.type",
                "entity.name.tag",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#993300"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string",
                "string.quoted",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#4D734D"
            }
        },
        {
            "name": "Headers & Highlights",
            "scope": [
                "markup.heading",
                "markup.heading.setext",
                "markup.heading.markdown",
                "punctuation.definition.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#CC3300",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call",
                "entity.name.method",
                "support.function.method",
                "meta.method-call",
                "entity.name.function.decorator",
                "meta.decorator"
            ],
            "settings": {
                "foreground": "#336666"
            }
        },
        {
            "name": "Classes & Types",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class",
                "support.type",
                "entity.other.inherited-class",
                "meta.class",
                "meta.type"
            ],
            "settings": {
                "foreground": "#7A527A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables & Parameters",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable",
                "variable.parameter",
                "variable.other",
                "variable.language",
                "meta.parameter-type"
            ],
            "settings": {
                "foreground": "#5C5C5C"
            }
        },
        {
            "name": "Numbers & Booleans",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other",
                "support.constant"
            ],
            "settings": {
                "foreground": "#8B5E5E"
            }
        },
        {
            "name": "Object Properties & Keys",
            "scope": [
                "meta.object-literal.key",
                "support.type.property-name",
                "variable.object.property",
                "entity.name.label",
                "string.unquoted.label"
            ],
            "settings": {
                "foreground": "#4D7A7A"
            }
        },
        {
            "name": "Operators & Punctuation",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.section",
                "punctuation.definition",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#6B6B47"
            }
        },
        {
            "name": "Markup Formatting",
            "scope": [
                "markup.bold",
                "markup.italic",
                "markup.strikethrough",
                "markup.underline",
                "markup.quote",
                "markup.list"
            ],
            "settings": {
                "foreground": "#804040"
            }
        },
        {
            "name": "Regex & Escapes",
            "scope": [
                "string.regexp",
                "constant.character.escape",
                "constant.character.escape.backslash"
            ],
            "settings": {
                "foreground": "#4D7A7A"
            }
        }
    ]
}