    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords & Storage",
            "scope": [
                "keyword",
                "storage",
                "storage.type",
                "entity.name.tag",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#FF9966"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string",
                "string.quoted",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#99CC99"
            }
        },
        {
            "name": "Headers & Highlights",
            "scope": [
                "markup.heading",
                "markup.heading.setext",
                "markup.heading.markdown",
                "punctuation.definition.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#FF99CC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call",
                "entity.name.method",
                "support.function.method",
                "meta.method-call",
                "entity.name.function.decorator",
                "meta.decorator"
            ],
            "settings": {
                "foreground": "#99CCCC"
            }
        },
        {
            "name": "Classes & Types",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class",
                "support.type",
                "entity.other.inherited-class",
                "meta.class",
                "meta.type"
            ],
            "settings": {
                "foreground": "#CC99CC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables & Parameters",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable",
                "variable.parameter",
                "variable.other",
                "variable.language",
                "meta.parameter-type"
            ],
            "settings": {
                "foreground": "#CCCCCC"
            }
        },
        {
            "name": "Numbers & Booleans",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other",
                "support.constant"
            ],
            "settings": {
                "foreground": "#CC9999"
            }
        },
        {
            "name": "Object Properties & Keys",
            "scope": [
                "meta.object-literal.key",
                "support.type.property-name",
                "variable.object.property",
                "entity.name.label",
                "string.unquoted.label"
            ],
            "settings": {
                "foreground": "#99CCCC"
            }
        },
        {
            "name": "Operators & Punctuation",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.section",
                "punctuation.definition",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#CCCC99"
            }
        },
        {
            "name": "Markup Formatting",
            "scope": [
                "markup.bold",
                "markup.italic",
                "markup.strikethrough",
                "markup.underline",
                "markup.quote",
                "markup.list"
            ],
            "settings": {
                "foreground": "#CC9999"
            }
        },
        {
            "name": "Regex & Escapes",
            "scope": [
                "string.regexp",
                "constant.character.escape",
                "constant.character.escape.backslash"
            ],
            "settings": {
                "foreground": "#99CCCC"
            }
        }
    ]
}